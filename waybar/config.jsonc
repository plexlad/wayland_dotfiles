{
	"layer": "top",
	"modules-left": ["clock"],
	"modules-center": ["hyprland/workspaces"],
	"module-right": ["battery"],
	"hyprland/workspaces": {
		"max-length": 50
	},
	"battery": {
		"format": "{capacity}% {icon}",
		"format-icons": ["1", "2", "3"]
	},
	"clock": {
		"format-alt": "{:%a, %d. %b %H:%M}"
	}
}
